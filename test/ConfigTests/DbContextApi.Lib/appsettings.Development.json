{
  "Apis": {
    "DbContextApi": {
      "ProjectName": "DbContextApi",
      "HttpsPort": 5501,
      "HttpPort": 5502,
      "Scopes": [
        "DbContextApi.*"
      ]
    }
  },
  "ScopeProperties": {
    "Enabled": true,
    "UserSource": "JwtNameClaim",
    "CopyHeaders": true,
    "CopyClaims": true,
    "AppendHostPath": true
  },
  "DbContexts": {
    "DbContext1": {
      "DatabaseProvider": "SqlServer",
      "ConnectionString": "Server=(localdb)\\MSSQLLocalDB;Database=HrTest;Trusted_Connection=True;MultipleActiveResultSets=True;",
      "Interceptor": {
        "Enabled": true,
        "IsInMemory": false,
        "IsolationLevel": "ReadCommitted",
        "ResetSqlServerIdentities": false,
        "ResetSqlServerSequences": false
      }
    },
    "DbContext2": {
      "DatabaseProvider": "Sqlite",
      "ConnectionString": "Data Source=hr.db",
      "Interceptor": {
        "Enabled": true,
        "IsInMemory": false
      }
    },
    "DbContext3": {
      "DatabaseProvider": "InMemory",
      "ConnectionString": null,
      "Interceptor": {
        "Enabled": true,
        "InstanceNameSource": "SessionId",
        "IsInMemory": true
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  }
}
