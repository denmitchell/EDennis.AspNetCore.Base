{
  "DbContexts": {
    "DbContext1": {
      "DatabaseProvider": "SqlServer",
      "ConnectionString": "Data Source=hr.db",
      "Interceptor": {
        "InstanceNameSource": [ "JwtNameClaim" ],
        "IsInMemory": false,
        "IsolationLevel": "ReadCommitted",
        "ResetSqlServerIdentities": false,
        "ResetSqlServerSequences": false
      }
    },
    "DbContext2": {
      "DatabaseProvider": "Sqlite",
      "ConnectionString": "Data Source=hr.db",
      "Interceptor": {
        "IsInMemory": false
      }
    },
    "DbContext3": {
      "DatabaseProvider": "InMemory",
      "ConnectionString": null,
      "Interceptor": {
        "InstanceNameSource": [ "SessionId" ],
        "IsInMemory": true
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  }
}
