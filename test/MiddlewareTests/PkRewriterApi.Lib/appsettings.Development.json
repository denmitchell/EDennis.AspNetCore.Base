{
  "Apis": {
    "PkRewrtierApi": {
      "ProjectName": "PkRewrtierApi",
      "HttpsPort": 5501,
      "HttpPort": 5502,
      "Scopes": [
        "PkRewrtierApi.*"
      ]
    }
  },
  "PkRewriter": {
    "Enabled": true,
    "BasePrefix": -999,
    "DeveloperNameEnvironmentVariable": "DeveloperName",
    "DeveloperPrefixes": {
      "moe": -501,
      "larry": -502,
      "curly": -503
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  }
}
