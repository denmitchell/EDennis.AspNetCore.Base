{
  "ScopedConfiguration": {
    "Enabled": true
  },
  "Apis": {
    "DbContextInterceptorApi": {
      "ProjectName": "DbContextInterceptorApi",
      "HttpsPort": 44386,
      "HttpPort": 60871,
      "Scopes": [
        "DbContextInterceptorApi.*"
      ]
    }
  },
  "ScopeProperties": {
    "Enabled": true,
    "UserSources": {
      "AuthenticatedUserSource": "JwtNameClaim",
      "UnauthenticatedUserSource": "XUserQueryString"
    },
    "CopyHeaders": false,
    "CopyClaims": false,
    "AppendHostPath": false
  },
  "DbContexts": {
    "AppDbContext": {
      "DatabaseProvider": "SqlServer",
      "ConnectionString": "Server=(localdb)\\MSSQLLocalDB;Database=DbContextInterceptorDb2;Trusted_Connection=True;MultipleActiveResultSets=True;",
      "Interceptor": {
        "Enabled": true,
        "IsInMemory": false,
        "IsolationLevel": "ReadCommitted",
        "ResetSqlServerIdentities": false,
        "ResetSqlServerSequences": false
      }
    }
  },
  "DeveloperName": "moe",
  "PkRewriter": {
    "Enabled": true,
    "BasePrefix": -999,
    "DeveloperNameEnvironmentVariable": "DeveloperName",
    "DeveloperPrefixes": {
      "moe": -501,
      "larry": -502,
      "curly": -503
    }
  },
  "Logging": {
    "Loggers": {
      "Default": {
        "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
        "MinimumLevel": {
          "Default": "Information",
          "Override": {
            "Microsoft.AspNetCore": "Warning",
            "Microsoft": "Warning",
            "System": "Warning"
          }
        },
        "WriteTo": [
          { "Name": "Console" },
          {
            "Name": "File",
            "Args": {
              "path": "log.txt",
              "rollingInterval": "Day"
            }
          }
        ],
        "Enrich": [ "FromLogContext" ],
        "Properties": {
          "Application": "ColorsApi"
        }
      },
      "ScopedTraceLogger": {
        "Enabled": true,
        "AssignmentKeySource": "User",
        "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
        "MinimumLevel": {
          "Default": "Debug",
          "Override": {
            "Microsoft.AspNetCore": "Warning",
            "Microsoft": "Warning",
            "System": "Warning"
          }
        },
        "WriteTo": [
          { "Name": "Console" },
          {
            "Name": "File",
            "Args": {
              "path": "scoped-log.txt",
              "rollingInterval": "Day"
            }
          }
        ],
        "Enrich": [ "FromLogContext" ],
        "Properties": {
          "Application": "DbContextInterceptorApi"
        }
      }
    }
  }
}
