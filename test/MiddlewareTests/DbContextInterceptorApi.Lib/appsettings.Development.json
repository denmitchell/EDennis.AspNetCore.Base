{
  "Apis": {
    "DbContextInterceptorApi": {
      "ProjectName": "DbContextInterceptorApi",
      "HttpsPort": 44386,
      "HttpPort": 60871,
      "Scopes": [
        "DbContextInterceptorApi.*"
      ]
    }
  },
  "DbContexts": {
    "AppDbContext": {
      "DatabaseProvider": "SqlServer",
      "ConnectionString": "Server=(localdb)\\MSSQLLocalDB;Database=HrTest;Trusted_Connection=True;MultipleActiveResultSets=True;",
      "Interceptor": {
        "Enabled": true,
        "InstanceNameSource": "XUserQueryString",
        "IsInMemory": false,
        "IsolationLevel": "ReadCommitted",
        "ResetSqlServerIdentities": false,
        "ResetSqlServerSequences": false
      }
    }
  },
  "ScopedConfiguration": {
    "Enabled": true
  },
  "DeveloperName": "moe",
  "PkRewriter": {
    "Enabled": true,
    "BasePrefix": -999,
    "DeveloperNameEnvironmentVariable": "DeveloperName",
    "DeveloperPrefixes": {
      "moe": -501,
      "larry": -502,
      "curly": -503
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  }
}
