{
  "Apis": {
    "DbContextInterceptorApi": {
      "ProjectName": "DbContextInterceptorApi",
      "HttpsPort": 44386,
      "HttpPort": 60871,
      "Scopes": [
        "DbContextInterceptorApi.*"
      ]
    }
  },
  "DbContexts": {
    "AppDbContext": {
      "DatabaseProvider": "SqlServer",
      "ConnectionString": "Server=(localdb)\\MSSQLLocalDB;Database=HrTest;Trusted_Connection=True;MultipleActiveResultSets=True;",
      "Interceptor": {
        "InstanceNameSource": "JwtNameClaim",
        "IsInMemory": false,
        "IsolationLevel": "ReadCommitted",
        "ResetSqlServerIdentities": false,
        "ResetSqlServerSequences": false
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  }
}
