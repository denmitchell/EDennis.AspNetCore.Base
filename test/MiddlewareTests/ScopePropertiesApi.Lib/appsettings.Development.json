{
  "Apis": {
    "ScopePropertiesApi": {
      "ProjectName": "ScopePropertiesApi",
      "HttpsPort": 53589,
      "HttpPort": 44390,
      "Scopes": [
        "ScopePropertiesApi.*"
      ]
    }
  },
  "ScopedConfiguration": {
    "Enabled": true
  },
  "ScopeProperties": {
    "Enabled": true,
    "UserSource": "JwtNameClaim",
    "CopyHeaders": false,
    "CopyClaims": false,
    "AppendHostPath": false
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  }
}
